public class TimeEntryLineItemTriggerHelper {
    
    Public static void getDataFromJira(List<Time_Entry_Line_Items__c> NewLineItems){
        
        map<String,String> jiraIdbyTELI = new map<String,String>();
        map<String,Jira_Task__c> jiraRecordsbyId = new map<String,Jira_Task__c>();
        for(Time_Entry_Line_Items__c tel : NewLineItems){
            if(tel.Jira_Task__c != null){
                jiraIdbyTELI.put(tel.Id,tel.Jira_Task__c);      
            }
        }
        if(jiraIdbyTELI.values().size() > 0){
            List<Jira_Task__c> JiraData = [SELECT Jira_Task__c.Project_Resource_Mapping__r.Project__C, Actual_Efforts__c,Actual_Hours_Utilised__c,Assigned_By_Name__c,Assigned_By__c,Description__c,Estimated_Efforts__c,Estimated_Start_Date__c,Extension_Reason__c,Id,Is_Overdue__c,Task_End_Date_Time__c,Task_End_Date__c,Task_Start_Date_Time__c,Task_Start_Date__c,Team_Lead__c,Team_Member_Name__c,Team_Member__c FROM Jira_Task__c where id in : jiraIdbyTELI.values()];  
            for(Jira_Task__c jt : JiraData){
                jiraRecordsbyId.put(jt.Id , jt);
            }
        }
        system.debug('jiraRecordsbyId'+jiraRecordsbyId);
        
        for(Time_Entry_Line_Items__c tel : NewLineItems){
            if(tel.Jira_Task__c != null){
                tel.Task_Description__c = jiraRecordsbyId.get(tel.Jira_Task__c).Description__c;
                tel.Allocated_Hours__c = jiraRecordsbyId.get(tel.Jira_Task__c).Estimated_Efforts__c;
                tel.Date_Of_Work__c = jiraRecordsbyId.get(tel.Jira_Task__c).Task_Start_Date__c;
                tel.Project__c = jiraRecordsbyId.get(tel.Jira_Task__c).Project_Resource_Mapping__r.Project__C;
                tel.Status__c = 'In Progress';
            }
        }
    }
    
